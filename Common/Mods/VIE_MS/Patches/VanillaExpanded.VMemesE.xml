<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationSequence">
		<operations>
			<!-- Associate precepts with their PreceptGenExtension. Sort alphabetically by PreceptGenDef. -->
			<li Class="PatchOperationAddModExtension">
				<xpath>/Defs/PreceptDef[defName="VME_Alcohol_Desired" or defName="VME_Alcohol_Demanded"]</xpath>
				<value>
					<li Class="TG.Ideo.PreceptGenExtension">
						<defs>
							<li>TG_Alcohol_Desired</li>
						</defs>
					</li>
				</value>
			</li>
			<li Class="PatchOperationAddModExtension">
				<xpath>/Defs/PreceptDef[defName="VME_Anonymity_Required"]</xpath>
				<value>
					<li Class="TG.Ideo.PreceptGenExtension">
						<defs>
							<li>TG_Anonymity_Required</li>
						</defs>
					</li>
				</value>
			</li>
			<li Class="PatchOperationAddModExtension">
				<xpath>/Defs/PreceptDef[defName="VME_AutomationEfficiency_Increased"]</xpath>
				<value>
					<li Class="TG.Ideo.PreceptGenExtension">
						<defs>
							<li>TG_AutomationEfficiency_Increased</li>
						</defs>
					</li>
				</value>
			</li>
			<li Class="PatchOperationAddModExtension">
				<xpath>/Defs/PreceptDef[defName="VME_BodyMod_OnlyBiological"]</xpath>
				<value>
					<li Class="TG.Ideo.PreceptGenExtension">
						<defs>
							<li>TG_BodyMod_OnlyBiological</li>
						</defs>
					</li>
				</value>
			</li>
			<li Class="PatchOperationAddModExtension">
				<xpath>/Defs/PreceptDef[defName="VME_Corruption_Essential"]</xpath>
				<value>
					<li Class="TG.Ideo.PreceptGenExtension">
						<defs>
							<li>TG_Corruption_Essential</li>
						</defs>
					</li>
				</value>
			</li>
		</operations>
	</Operation>
</Patch>
