<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationSequence">
		<operations>
			<!-- Associate precepts with their PreceptGenExtension. Sort alphabetically by PreceptGenDef. -->
			<li Class="PatchOperationAddModExtension">
				<xpath>/Defs/PreceptDef[defName="AnimalConnection_Strong"]</xpath>
				<value>
					<li Class="TG.Ideo.PreceptGenExtension">
						<def>TG_AnimalConnection_Strong</def>
					</li>
				</value>
			</li>
			<li Class="PatchOperationAddModExtension">
				<xpath>/Defs/PreceptDef[defName="Blindness_Respected" or defName="Blindness_Elevated" or
					defName="Blindness_Sublime"]
				</xpath>
				<value>
					<li Class="TG.Ideo.PreceptGenExtension">
						<def>TG_Blindness_Respected</def>
					</li>
				</value>
			</li>
			<li Class="PatchOperationAddModExtension">
				<xpath>/Defs/PreceptDef[defName="NutrientPasteEating_DontMind"]</xpath>
				<value>
					<li Class="TG.Ideo.PreceptGenExtension">
						<def>TG_NutrientPasteEating_DontMind</def>
					</li>
				</value>
			</li>
			<li Class="PatchOperationAddModExtension">
				<xpath>/Defs/PreceptDef[defName="Blind_Psysense"]
				</xpath>
				<value>
					<li Class="TG.Ideo.PreceptGenExtension">
						<def>TG_PsyInclined</def>
					</li>
				</value>
			</li>
		</operations>
	</Operation>
</Patch>
