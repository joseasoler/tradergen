<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationSequence">
		<operations>
			<!-- Associate precepts with their PreceptGenExtension. Sort alphabetically by PreceptGenDef. -->
			<li Class="PatchOperationAddModExtension">
				<xpath>/Defs/PreceptDef[defName="AnimalConnection_Strong"]</xpath>
				<value>
					<li Class="TG.Ideo.PreceptGenExtension">
						<defs>
							<li>TG_AnimalConnection_Strong</li>
						</defs>
					</li>
				</value>
			</li>
			<li Class="PatchOperationAddModExtension">
				<xpath>/Defs/PreceptDef[defName="Blindness_Respected" or defName="Blindness_Elevated" or
					defName="Blindness_Sublime"]
				</xpath>
				<value>
					<li Class="TG.Ideo.PreceptGenExtension">
						<defs>
							<li>TG_Blindness_Respected</li>
						</defs>
					</li>
				</value>
			</li>
			<li Class="PatchOperationAddModExtension">
				<xpath>/Defs/PreceptDef[defName="BodyMod_Approved"]
				</xpath>
				<value>
					<li Class="TG.Ideo.PreceptGenExtension">
						<defs>
							<li>TG_BodyMod_Approved</li>
						</defs>
					</li>
				</value>
			</li>
			<li Class="PatchOperationAddModExtension">
				<xpath>/Defs/PreceptDef[defName="BodyMod_Disapproved" or defName="BodyMod_Abhorrent"]
				</xpath>
				<value>
					<li Class="TG.Ideo.PreceptGenExtension">
						<defs>
							<li>TG_BodyMod_Disapproved</li>
						</defs>
					</li>
				</value>
			</li>
			<li Class="PatchOperationAddModExtension">
				<xpath>/Defs/PreceptDef[defName="Cannibalism_Disapproved" or defName="Cannibalism_Horrible" or defName="Cannibalism_Abhorrent"]
				</xpath>
				<value>
					<li Class="TG.Ideo.PreceptGenExtension">
						<defs>
							<li>TG_Cannibalism_Disapproved</li>
						</defs>
					</li>
				</value>
			</li>
			<li Class="PatchOperationAddModExtension">
				<xpath>/Defs/PreceptDef[defName="Cannibalism_Preferred" or defName="Cannibalism_RequiredStrong" or defName="Cannibalism_RequiredRavenous"]
				</xpath>
				<value>
					<li Class="TG.Ideo.PreceptGenExtension">
						<defs>
							<li>TG_Cannibalism_Preferred</li>
						</defs>
					</li>
				</value>
			</li>
			<li Class="PatchOperationAddModExtension">
				<xpath>/Defs/PreceptDef[defName="NutrientPasteEating_DontMind"]</xpath>
				<value>
					<li Class="TG.Ideo.PreceptGenExtension">
						<defs>
							<li>TG_NutrientPasteEating_DontMind</li>
						</defs>
					</li>
				</value>
			</li>
			<li Class="PatchOperationAddModExtension">
				<xpath>/Defs/PreceptDef[defName="Blind_Psysense"]
				</xpath>
				<value>
					<li Class="TG.Ideo.PreceptGenExtension">
						<defs>
							<li>TG_PsyInclined</li>
						</defs>
					</li>
				</value>
			</li>
		</operations>
	</Operation>
</Patch>
