<?xml version="1.0" encoding="utf-8" ?>
<!-- Do not generate vanilla orbital traders. -->
<Patch>
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/TraderKindDef[defName="Orbital_BulkGoods" or defName="Orbital_CombatSupplier" or
			defName="Orbital_Exotic"]
		</xpath>
		<value>
			<commonality>0</commonality>
		</value>
	</Operation>
</Patch>
