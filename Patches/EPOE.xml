<?xml version="1.0" encoding="UTF-8"?>
<!--
Disable Prostheses and Textiles Trader as their role is fulfilled by some specializations.
-->
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Expanded Prosthetics and Organ Engineering</li>
			<li>Expanded Prosthetics and Organ Engineering - Forked</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAddModExtension">
					<xpath>/Defs/TraderKindDef[defName="ProsthesesTrader"]</xpath>
					<value>
						<li Class="TG.GenExtension"/>
					</value>
				</li>
				<li Class="PatchOperationAddModExtension">
					<xpath>/Defs/TraderKindDef[defName="TextilesTrader"]</xpath>
					<value>
						<li Class="TG.GenExtension"/>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>