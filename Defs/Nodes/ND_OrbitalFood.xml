<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<TG.NodeDef>
		<defName>ND_OrbitalFood</defName>
		<generators>
			<!--
			Sells canned food when Vanilla Cooking is enabled. Raw vanilla food otherwise.
			See VanillaExpanded.VCookE.xml patch for details.
			-->
			<li Class="StockGenerator_SingleDef" MayRequire="VanillaExpanded.VWE">
				<thingDef>VWE_MeleeWeapon_Cleaver</thingDef>
				<countRange>-1~2</countRange>
			</li>
			<!-- Purchases -->
			<li Class="TG.StockGen.FoodRaw"/>
			<li Class="StockGenerator_Category" MayRequire="VanillaExpanded.VCookE">
				<categoryDef>VCE_Condiments</categoryDef>
			</li>
		</generators>
		<next Class="TG.Next.ChooseN">
			<nodes>
				<li>
					<node>ND_OrbitalFoodSurvivalist</node>
					<commonality>3</commonality>
				</li>
				<li>
					<node>ND_OrbitalFoodGourmet</node>
					<commonality>3</commonality>
				</li>
				<li>
					<node>ND_OrbitalFoodGluttonous</node>
					<commonality>3</commonality>
				</li>
				<li>
					<node>ND_OrbitalFoodRecreational</node>
					<commonality>3</commonality>
				</li>
				<li>
					<node>ND_OrbitalFoodInsectoid</node>
					<commonality>1</commonality>
				</li>
			</nodes>
		</next>
	</TG.NodeDef>
	<TG.NodeDef>
		<defName>ND_OrbitalFoodSurvivalist</defName>
		<generators>
			<!-- Sells -->
			<li Class="StockGenerator_SingleDef">
				<thingDef>Pemmican</thingDef>
				<totalPriceRange>625~1125</totalPriceRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>MealSurvivalPack</thingDef>
				<totalPriceRange>1000~2000</totalPriceRange>
			</li>
			<li Class="StockGenerator_SingleDef" MayRequire="VanillaExpanded.VCookE">
				<thingDef>VCE_Flour</thingDef>
				<totalPriceRange>1000~2000</totalPriceRange>
			</li>
			<li Class="StockGenerator_SingleDef" MayRequire="VanillaExpanded.VCookE">
				<thingDef>VCE_Salt</thingDef>
				<totalPriceRange>300~600</totalPriceRange>
			</li>
		</generators>
	</TG.NodeDef>
	<TG.NodeDef>
		<defName>ND_OrbitalFoodGourmet</defName>
		<generators>
			<!-- Sells -->
			<li Class="StockGenerator_Category" MayRequire="VanillaExpanded.VCookE">
				<categoryDef>VCE_Condiments</categoryDef>
				<thingDefCountRange>2~4</thingDefCountRange>
				<totalPriceRange>800~1200</totalPriceRange>
			</li>
			<!--
			Vanilla meals and ThingDefs that inherit from them have their tradeability set so they can only be purchased by
			traders, not sold. StockGenerator_MultiDef is used instead of TG.StockGen.MultiDef, as the former lacks this test.
			-->
			<li Class="StockGenerator_MultiDef">
				<thingDefs>
					<li>MealLavish</li>
					<li MayRequire="VanillaExpanded.VCookE">VCE_MealGourmet</li>
					<li MayRequire="VanillaExpanded.VCookE">VCE_CookedSoupLavish</li>
					<li MayRequire="VanillaExpanded.VCookE">VCE_CookedSoupGourmet</li>
					<li MayRequire="VanillaExpanded.VCookE">VCE_LavishBake</li>
					<li MayRequire="VanillaExpanded.VCookE">VCE_GourmetBake</li>
					<li MayRequire="VanillaExpanded.VCookEStews">VCE_CookedStewLavish</li>
				</thingDefs>
				<totalPriceRange>1200~2000</totalPriceRange>
			</li>
			<li Class="StockGenerator_MultiDef">
				<thingDefs>
					<li>MealLavish</li>
					<li MayRequire="VanillaExpanded.VCookE">VCE_MealGourmet</li>
					<li MayRequire="VanillaExpanded.VCookE">VCE_CookedSoupLavish</li>
					<li MayRequire="VanillaExpanded.VCookE">VCE_CookedSoupGourmet</li>
					<li MayRequire="VanillaExpanded.VCookE">VCE_LavishBake</li>
					<li MayRequire="VanillaExpanded.VCookE">VCE_GourmetBake</li>
					<li MayRequire="VanillaExpanded.VCookEStews">VCE_CookedStewLavish</li>
				</thingDefs>
				<totalPriceRange>1200~2000</totalPriceRange>
			</li>
		</generators>
	</TG.NodeDef>
	<TG.NodeDef>
		<defName>ND_OrbitalFoodGluttonous</defName>
		<generators>
			<!-- Sells -->
			<li Class="TG.StockGen.FoodGluttonous">
				<thingDefCountRange>3~5</thingDefCountRange>
				<totalPriceRange>1000~1800</totalPriceRange>
			</li>
			<li Class="TG.StockGen.MultiDef" MayRequire="VanillaExpanded.VCookE">
				<thingDefs>
					<li>VCE_ChocolateSyrup</li>
					<li>VCE_InsectJellyPreserves</li>
					<li>VCE_RawSugar</li>
				</thingDefs>
				<thingDefCountRange>1~2</thingDefCountRange>
				<totalPriceRange>500~800</totalPriceRange>
			</li>
		</generators>
	</TG.NodeDef>
	<TG.NodeDef>
		<defName>ND_OrbitalFoodRecreational</defName>
		<generators>
			<li Class="TG.StockGen.Alcohol">
				<thingDefCountRange>2~3</thingDefCountRange>
				<totalPriceRange>1000~2200</totalPriceRange>
			</li>
			<li Class="StockGenerator_Category" MayRequire="VanillaExpanded.VBrewE">
				<categoryDef>VBE_DrinksNonAlcoholic</categoryDef>
				<excludedThingDefs>
					<li MayRequire="VanillaExpanded.VBrewECandT">VBE_CannibalCoffee</li>
				</excludedThingDefs>
				<thingDefCountRange>2~3</thingDefCountRange>
				<totalPriceRange>1000~2200</totalPriceRange>
			</li>
			<li Class="StockGenerator_Category" MayRequire="VanillaExpanded.VCookE">
				<categoryDef>VCE_Condiments</categoryDef>
				<thingDefCountRange>1~2</thingDefCountRange>
				<totalPriceRange>300~700</totalPriceRange>
			</li>
			<li Class="TG.StockGen.MultiDef">
				<thingDefs>
					<li MayRequire="VanillaExpanded.VBrewE">VBE_Cigars</li>
					<li MayRequire="VanillaExpanded.VBrewE">VBE_Cigarettes</li>
					<li>PsychiteTea</li>
					<li>SmokeleafJoint</li>
				</thingDefs>
				<thingDefCountRange>2~3</thingDefCountRange>
				<totalPriceRange>400~900</totalPriceRange>
			</li>
		</generators>
	</TG.NodeDef>
	<TG.NodeDef>
		<defName>ND_OrbitalFoodInsectoid</defName>
		<generators>
			<!-- Sells -->
			<li Class="TG.StockGen.InsectMeat">
				<thingDefCountRange>1</thingDefCountRange>
				<totalPriceRange>1200~1800</totalPriceRange>
			</li>
			<li Class="TG.StockGen.MultiDef">
				<thingDefs>
					<li>InsectJelly</li>
					<li MayRequire="VanillaExpanded.VCookE">VCE_InsectJellyPreserves</li>
					<li MayRequire="sarg.alphaanimals">AA_BlackInsectJelly</li>
					<li MayRequire="OskarPotocki.VFE.Insectoid">VFEI_RoyalInsectJelly</li>
					<li MayRequire="OskarPotocki.VFE.Insectoid">VFEI_InsectMilk</li>
					<li MayRequire="OskarPotocki.VFE.Insectoid">VFEI_EggInsectoidUnfertilized</li>
				</thingDefs>
				<thingDefCountRange>2~3</thingDefCountRange>
				<totalPriceRange>400~850</totalPriceRange>
			</li>
			<li Class="StockGenerator_SingleDef" MayRequire="OskarPotocki.VFE.Insectoid">
				<thingDef>VFEI_BioengineeringIncubator</thingDef>
				<countRange>0~2</countRange>
			</li>
			<li Class="StockGenerator_SingleDef" MayRequire="OskarPotocki.VFE.Insectoid">
				<thingDef>VFEI_DroneGenome</thingDef>
				<countRange>2~8</countRange>
			</li>
			<li Class="StockGenerator_SingleDef" MayRequire="OskarPotocki.VFE.Insectoid">
				<thingDef>VFEI_RoyalGenome</thingDef>
				<countRange>2~8</countRange>
			</li>
			<li Class="TG.StockGen.AnimalMultiDef" MayRequire="OskarPotocki.VFE.Insectoid">
				<pawnKindDefs>
					<li>VFEI_VatGrownMilkbeetle</li>
					<li>VFEI_VatGrownPrincess</li>
					<li>VFEI_VatgrownRoyalLarva</li>
					<li>VFEI_VatGrownRoyalMaggot</li>
				</pawnKindDefs>
				<newborn>true</newborn>
				<kindCountRange>1~2</kindCountRange>
				<totalPriceRange>500~800</totalPriceRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Kibble</thingDef>
				<totalPriceRange>1200~1600</totalPriceRange>
			</li>
			<li Class="StockGenerator_SingleDef" MayRequire="VanillaExpanded.VWE">
				<thingDef>VWE_Tool_Whip</thingDef>
				<countRange>-1~2</countRange>
			</li>
			<!-- Purchases -->
			<li Class="StockGenerator_Animals">
				<tradeTagsBuy>
					<li>AnimalInsect</li>
					<li>AnimalExotic</li>
				</tradeTagsBuy>
			</li>
		</generators>
	</TG.NodeDef>
</Defs>
