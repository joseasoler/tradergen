<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<!-- Small independent traders. -->
	<TG.NodeDef>
		<defName>ND_OrbitalIndependentSmall</defName>
		<next Class="TG.Next.All">
			<nodes>
				<li>ND_Orbital</li>
				<li>ND_OrbitalIndependentSmallGroups</li>
			</nodes>
		</next>
	</TG.NodeDef>
	<TG.NodeDef>
		<defName>ND_OrbitalIndependentSmallGroups</defName>
		<next Class="TG.Next.ChooseN">
			<nodes>
				<li>
					<node>ND_OrbitalBulk</node>
				</li>
				<li>
					<node>ND_OrbitalCombat</node>
					<commonality>2</commonality>
				</li>
				<li>
					<node>ND_OrbitalFood</node>
					<commonality>2</commonality>
				</li>
				<li>
					<node>ND_OrbitalFromStuff</node>
					<commonality>2</commonality>
				</li>
			</nodes>
		</next>
	</TG.NodeDef>
	<!-- Medium independent traders. -->
	<TG.NodeDef>
		<defName>ND_OrbitalIndependentMedium</defName>
		<next Class="TG.Next.All">
			<nodes>
				<li>ND_Orbital</li>
				<li>ND_OrbitalIndependentMediumGroups</li>
			</nodes>
		</next>
	</TG.NodeDef>
	<TG.NodeDef>
		<defName>ND_OrbitalIndependentMediumGroups</defName>
		<next Class="TG.Next.ChooseGroup">
			<nodeGroups>
				<li>
					<nodes>
						<li>ND_OrbitalBulk</li>
						<li>ND_OrbitalBulk</li>
					</nodes>
					<commonality>2</commonality>
				</li>
				<li>
					<nodes>
						<li>ND_OrbitalBulk</li>
						<li>ND_OrbitalCombat</li>
					</nodes>
				</li>
				<li>
					<nodes>
						<li>ND_OrbitalBulk</li>
						<li>ND_OrbitalFromStuff</li>
					</nodes>
					<commonality>2</commonality>
				</li>
				<li>
					<nodes>
						<li>ND_OrbitalCombat</li>
						<li>ND_OrbitalCombat</li>
					</nodes>
				</li>
				<li>
					<nodes>
						<li>ND_OrbitalCombat</li>
						<li>ND_OrbitalFood</li>
					</nodes>
				</li>
				<li>
					<nodes>
						<li>ND_OrbitalFood</li>
						<li>ND_OrbitalFood</li>
					</nodes>
					<commonality>2</commonality>
				</li>
				<li>
					<nodes>
						<li>ND_OrbitalFood</li>
						<li>ND_OrbitalFromStuff</li>
					</nodes>
				</li>
			</nodeGroups>
		</next>
	</TG.NodeDef>
	<!-- Large independent traders. -->
	<TG.NodeDef>
		<defName>ND_OrbitalIndependentLarge</defName>
		<next Class="TG.Next.All">
			<nodes>
				<li>ND_Orbital</li>
				<li>ND_OrbitalIndependentLargeGroups</li>
			</nodes>
		</next>
	</TG.NodeDef>
	<TG.NodeDef>
		<defName>ND_OrbitalIndependentLargeGroups</defName>
		<next Class="TG.Next.ChooseGroup">
			<nodeGroups>
				<li>
					<nodes>
						<li>ND_OrbitalBulk</li>
						<li>ND_OrbitalBulk</li>
						<li>ND_OrbitalFood</li>
					</nodes>
					<commonality>3</commonality>
				</li>
				<li>
					<nodes>
						<li>ND_OrbitalBulk</li>
						<li>ND_OrbitalCombat</li>
						<li>ND_OrbitalCombat</li>
					</nodes>
				</li>
				<li>
					<nodes>
						<li>ND_OrbitalBulk</li>
						<li>ND_OrbitalFood</li>
						<li>ND_OrbitalFromStuff</li>
					</nodes>
					<commonality>2</commonality>
				</li>
				<li>
					<nodes>
						<li>ND_OrbitalBulk</li>
						<li>ND_OrbitalFromStuff</li>
						<li>ND_OrbitalFromStuff</li>
					</nodes>
					<commonality>2</commonality>
				</li>
				<li>
					<nodes>
						<li>ND_OrbitalCombat</li>
						<li>ND_OrbitalCombat</li>
						<li>ND_OrbitalCombat</li>
					</nodes>
				</li>
				<li>
					<nodes>
						<li>ND_OrbitalCombat</li>
						<li>ND_OrbitalCombat</li>
						<li>ND_OrbitalFood</li>
					</nodes>
				</li>
				<li>
					<nodes>
						<li>ND_OrbitalCombat</li>
						<li>ND_OrbitalCombat</li>
						<li>ND_OrbitalFromStuff</li>
					</nodes>
				</li>
				<li>
					<nodes>
						<li>ND_OrbitalFood</li>
						<li>ND_OrbitalFood</li>
						<li>ND_OrbitalFood</li>
					</nodes>
					<commonality>2</commonality>
				</li>
				<li>
					<nodes>
						<li>ND_OrbitalFood</li>
						<li>ND_OrbitalFromStuff</li>
						<li>ND_OrbitalFromStuff</li>
					</nodes>
				</li>
			</nodeGroups>
		</next>
	</TG.NodeDef>
	<!-- Traders with specific and rare combinations of stock. -->
	<TG.NodeDef>
		<defName>ND_OrbitalIndependentUncommon</defName>
		<next Class="TG.Next.All">
			<nodes>
				<li>ND_Orbital</li>
				<li>ND_OrbitalIndependentUncommonGroups</li>
			</nodes>
		</next>
	</TG.NodeDef>
	<TG.NodeDef>
		<defName>ND_OrbitalIndependentUncommonGroups</defName>
		<next Class="TG.Next.ChooseGroup">
			<nodeGroups>
				<li>
					<nodes>
						<li>ND_OrbitalUncommonHiTech</li>
						<li>ND_OrbitalCombatHiTech</li>
						<!-- ToDo implants -->
					</nodes>
					<commonality>20</commonality>
				</li>
				<li MayRequire="OskarPotocki.VFE.Insectoid">
					<nodes>
						<li>ND_OrbitalCombatInsectoid</li>
						<li>ND_OrbitalFoodInsectoid</li>
						<li>ND_OrbitalFromStuffInsect</li>
						<li MayRequire="sarg.rimbees">ND_OrbitalFromStuffWaxy</li>
					</nodes>
					<commonality>10</commonality>
				</li>
				<li>
					<nodes MayRequire="sarg.alphaanimals">
						<li>ND_OrbitalUncommonBiomaterials</li>
					</nodes>
					<commonality>10</commonality>
				</li>
			</nodeGroups>
		</next>
	</TG.NodeDef>
</Defs>