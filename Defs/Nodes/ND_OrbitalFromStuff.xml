<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<TG.NodeDef>
		<defName>ND_OrbitalFromStuff</defName>
		<next Class="TG.Next.ListWithoutRepetition">
			<nodes>
				<li>
					<node>ND_OrbitalFromStuffMetallic</node>
					<commonality>6</commonality>
				</li>
				<li>
					<node>ND_OrbitalFromStuffWoody</node>
					<commonality>2</commonality>
				</li>
				<li>
					<node>ND_OrbitalFromStuffStony</node>
					<commonality>4</commonality>
				</li>
				<li>
					<node>ND_OrbitalFromStuffFabric</node>
					<commonality>6</commonality>
				</li>
				<li>
					<node>ND_OrbitalFromStuffLeathery</node>
					<commonality>6</commonality>
				</li>
				<li MayRequire="kikohi.jewelry">
					<node>ND_OrbitalFromStuffGemstones</node>
					<commonality>3</commonality>
				</li>
				<!-- ToDo
				<li MayRequire="sarg.rimbees">
					<node>ND_OrbitalFromStuffWaxy</node>
					<commonality>1</commonality>
				</li>
				-->
			</nodes>
		</next>
	</TG.NodeDef>
	<TG.NodeDef>
		<defName>ND_OrbitalFromStuffMetallic</defName>
		<generators>
			<!-- Sells -->
			<li Class="TG.StockGen.RandomStuffFromCategory">
				<stuffCategoryDef>Metallic</stuffCategoryDef>
				<maxTechLevelGenerate>Spacer</maxTechLevelGenerate>
				<excludeThingDefs>
					<!-- Chitin is handled separately. -->
					<li MayRequire="OskarPotocki.VFE.Insectoid">VFEI_Chitin</li>
				</excludeThingDefs>
				<stuffCountRange>300~700</stuffCountRange>
				<thingDefCountRange>4~7</thingDefCountRange>
				<countRange>1~2</countRange>
			</li>
			<li Class="TG.StockGen.RandomStuffFromCategory">
				<stuffCategoryDef>Metallic</stuffCategoryDef>
				<maxTechLevelGenerate>Spacer</maxTechLevelGenerate>
				<excludeThingDefs>
					<!-- Chitin is handled separately. -->
					<li MayRequire="OskarPotocki.VFE.Insectoid">VFEI_Chitin</li>
				</excludeThingDefs>
				<stuffCountRange>300~700</stuffCountRange>
				<thingDefCountRange>4~7</thingDefCountRange>
				<countRange>1~2</countRange>
			</li>
		</generators>
	</TG.NodeDef>
