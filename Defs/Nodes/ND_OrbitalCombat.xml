<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<TG.NodeDef>
		<defName>ND_OrbitalCombat</defName>
		<generators>
			<!-- Sells -->
			<li Class="StockGenerator_SingleDef">
				<thingDef>MedicineIndustrial</thingDef>
				<countRange>30~50</countRange>
			</li>
			<li Class="StockGenerator_ReinforcedBarrels">
				<countRange>2~4</countRange>
			</li>
			<li Class="StockGenerator_Tag">
				<tradeTag>MortarShell</tradeTag>
				<thingDefCountRange>1~2</thingDefCountRange>
				<countRange>5~10</countRange>
			</li>
			<li Class="TG.StockGen.Armor">
				<minTechLevelGenerate>Industrial</minTechLevelGenerate>
				<maxTechLevelGenerate>Spacer</maxTechLevelGenerate>
				<excludeTradeTags>
					<li>HiTechArmor</li>
					<!-- VFEP_Apparel_Privateer lacks the HiTechArmor trade tag used by other power armor items. -->
					<li MayRequire="OskarPotocki.VFE.Pirates">PrivateerArmor</li>
					<!--
					Some Vanilla Armor Expanded power armor lacks the HiTechArmor tradeTag. A new tradeTag is patched into them
					to avoid changing other behavior.
					-->
					<li MayRequire="VanillaExpanded.VARME">TG_PatchedHiTechArmor</li>
				</excludeTradeTags>
				<thingDefCountRange>2~4</thingDefCountRange>
				<countRange>1~3</countRange>
			</li>
			<li Class="TG.StockGen.MultiDef">
				<thingDefs>
					<li>Apparel_ShieldBelt</li>
					<li>Apparel_SmokepopBelt</li>
					<li MayRequire="VanillaExpanded.VAEAccessories">VAEA_Apparel_RangedShieldBelt</li>
				</thingDefs>
				<thingDefCountRange>-1~1</thingDefCountRange>
				<countRange>1~2</countRange>
			</li>
			<li Class="TG.StockGen.WeaponMelee">
				<minTechLevelGenerate>Medieval</minTechLevelGenerate>
				<maxTechLevelGenerate>Spacer</maxTechLevelGenerate>
				<excludeWeaponTags>
					<li MayRequire="VanillaExpanded.VWE">ToolDecent</li>
					<li MayRequire="OskarPotocki.VFE.Pirates">WarcasketMelee</li>
				</excludeWeaponTags>
				<thingDefCountRange>3~5</thingDefCountRange>
				<countRange>1~2</countRange>
			</li>
			<li Class="TG.StockGen.WeaponRanged">
				<minTechLevelGenerate>Industrial</minTechLevelGenerate>
				<maxTechLevelGenerate>Spacer</maxTechLevelGenerate>
				<excludeWeaponTags>
					<li MayRequire="VanillaExpanded.VWE">ToolDecent</li>
					<li MayRequire="OskarPotocki.VFE.Pirates">WarcasketAll</li>
				</excludeWeaponTags>
				<thingDefCountRange>7~11</thingDefCountRange>
				<countRange>1~2</countRange>
			</li>
			<!-- Purchases -->
			<li Class="StockGenerator_Category">
				<categoryDef>Drugs</categoryDef>
			</li>
			<li Class="StockGenerator_Tag">
				<tradeTag>MortarShell</tradeTag>
			</li>
		</generators>
		<next Class="TG.Next.ListWithoutRepetition">
			<nodes>
				<li>
					<node>ND_OrbitalCombatAnimal</node>
					<commonality>3</commonality>
				</li>
				<li>
					<node>ND_OrbitalCombatArmor</node>
					<commonality>3</commonality>
				</li>
				<li>
					<node>ND_OrbitalCombatInsectoid</node>
					<commonality>2</commonality>
				</li>
				<li>
					<node>ND_OrbitalCombatOrdnance</node>
					<commonality>2</commonality>
				</li>
				<li>
					<node>ND_OrbitalCombatHiTech</node>
					<commonality>2</commonality>
				</li>
			</nodes>
		</next>
	</TG.NodeDef>
	<TG.NodeDef>
		<defName>ND_OrbitalCombatAnimal</defName>
		<generators>
			<!-- Sells -->
			<li Class="StockGenerator_Animals">
				<maxWildness>0.70</maxWildness>
				<checkTemperature>false</checkTemperature>
				<tradeTagsSell>
					<li>AnimalFighter</li>
				</tradeTagsSell>
				<tradeTagsBuy>
					<li>AnimalUncommon</li>
					<li>AnimalExotic</li>
				</tradeTagsBuy>
				<kindCountRange>2~4</kindCountRange>
				<countRange>6~9</countRange>
			</li>
			<li Class="TG.StockGen.MultiDef" MayRequire="VanillaExpanded.VWE">
				<thingDefs>
					<li>VWE_Bow_Compound</li>
					<li MayRequire="VanillaExpanded.VAEAccessories">VAEA_Apparel_Quiver</li>
				</thingDefs>
				<thingDefCountRange>2</thingDefCountRange>
				<countRange>2~4</countRange>
			</li>
			<li Class="StockGenerator_SingleDef" MayRequire="VanillaExpanded.VWENL">
				<thingDef>VWE_Gun_DartGun</thingDef>
				<countRange>-2~2</countRange>
			</li>
			<li Class="TG.StockGen.MultiDef" MayRequire="VanillaExpanded.VARME">
				<thingDefs>
					<li>VAE_Apparel_GhillieSuit</li>
					<li>VAE_Headgear_GhillieHood</li>
				</thingDefs>
				<thingDefCountRange>2</thingDefCountRange>
				<countRange>1~2</countRange>
			</li>
			<li Class="StockGenerator_SingleDef" MayRequire="OskarPotocki.VanillaFactionsExpanded.SettlersModule">
				<thingDef>VFES_Gun_HuntingRifle</thingDef>
				<countRange>1~4</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Gun_BoltActionRifle</thingDef>
				<countRange>1~2</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Gun_SniperRifle</thingDef>
				<countRange>-1~1</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Kibble</thingDef>
				<totalPriceRange>500~1000</totalPriceRange>
			</li>
			<li Class="TG.StockGen.ExoticAnimalPart">
				<thingDefCountRange>0~2</thingDefCountRange>
				<countRange>1</countRange>
			</li>
		</generators>
	</TG.NodeDef>
	<TG.NodeDef>
		<defName>ND_OrbitalCombatArmor</defName>
		<generators>
			<!-- Sells -->
			<li Class="TG.StockGen.Armor">
				<minTechLevelGenerate>Industrial</minTechLevelGenerate>
				<tradeTags>
					<li>HiTechArmor</li>
					<!-- VFEP_Apparel_Privateer lacks the HiTechArmor trade tag used by other power armor items. -->
					<li MayRequire="OskarPotocki.VFE.Pirates">PrivateerArmor</li>
					<!--
					Some Vanilla Armor Expanded power armor lacks the HiTechArmor tradeTag. A new tradeTag is patched into them
					to avoid changing other unrelated behavior.
					-->
					<li MayRequire="VanillaExpanded.VARME">TG_PatchedHiTechArmor</li>
				</tradeTags>
				<thingDefCountRange>2~4</thingDefCountRange>
				<countRange>1~2</countRange>
			</li>
			<li Class="TG.StockGen.MultiDef">
				<thingDefs>
					<li>Apparel_ShieldBelt</li>
					<li>Apparel_SmokepopBelt</li>
					<li MayRequire="VanillaExpanded.VAEAccessories">VAEA_Apparel_RangedShieldBelt</li>
				</thingDefs>
				<thingDefCountRange>1~3</thingDefCountRange>
				<countRange>2~3</countRange>
			</li>
			<li Class="StockGenerator_SingleDef" MayRequire="OskarPotocki.VanillaFactionsExpanded.SettlersModule">
				<thingDef>Apparel_PsychicFoilHelmet</thingDef>
				<countRange>-1~2</countRange>
			</li>
		</generators>
	</TG.NodeDef>
	<TG.NodeDef>
		<defName>ND_OrbitalCombatInsectoid</defName>
		<generators>
			<!--
			Sells vanilla insectoids when VFE.Insectoid is not found. Check Patch_ND_OrbitalCombatInsectoid for details.
			-->
			<li Class="StockGenerator_SingleDef" MayRequire="OskarPotocki.VFE.Insectoid">
				<thingDef>VFEI_WarriorGenome</thingDef>
				<countRange>2~8</countRange>
			</li>
			<li Class="TG.StockGen.MultiDef" MayRequire="OskarPotocki.VFE.Insectoid">
				<thingDefs>
					<li>VFEI_DroneGenome</li>
					<li>VFEI_RoyalGenome</li>
					<li MayRequire="sarg.alphaanimals">VFEI_BlackHiveGenome</li>
				</thingDefs>
				<thingDefCountRange>1~3</thingDefCountRange>
				<countRange>2~6</countRange>
			</li>
			<li Class="StockGenerator_SingleDef" MayRequire="OskarPotocki.VFE.Insectoid">
				<thingDef>VFEI_BioengineeringIncubator</thingDef>
				<countRange>0~2</countRange>
			</li>
			<li Class="StockGenerator_SingleDef" MayRequire="OskarPotocki.VFE.Insectoid">
				<thingDef>VFEI_SonicInfestationRepeller</thingDef>
				<countRange>-2~1</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Kibble</thingDef>
				<totalPriceRange>400~800</totalPriceRange>
			</li>
			<li Class="TG.StockGen.AnimalMultiDef" MayRequire="OskarPotocki.VFE.Insectoid">
				<pawnKindDefs>
					<li>VFEI_VatGrownMegascarab</li>
					<li>VFEI_VatgrownMegaspider</li>
					<li>VFEI_VatGrownSpelopede</li>
					<li>VFEI_VatGrownWargling</li>
					<li>VFEI_VatGrownGigascorpion</li>
					<li>VFEI_VatGrownMegapede</li>
					<li>VFEI_VatGrownGigalocust</li>
				</pawnKindDefs>
				<newborn>true</newborn>
				<kindCountRange>3~4</kindCountRange>
				<totalPriceRange>700~1900</totalPriceRange>
			</li>
			<!-- Purchases -->
			<li Class="StockGenerator_Animals">
				<tradeTagsBuy>
					<li>AnimalInsect</li>
					<li>AnimalExotic</li>
				</tradeTagsBuy>
			</li>
			<li Class="TG.StockGen.MultiDef">
				<thingDefs>
					<li>InsectJelly</li>
					<li MayRequire="VanillaExpanded.VCookE">VCE_InsectJellyPreserves</li>
					<li MayRequire="sarg.alphaanimals">AA_BlackInsectJelly</li>
					<li MayRequire="OskarPotocki.VFE.Insectoid">VFEI_RoyalInsectJelly</li>
					<li MayRequire="OskarPotocki.VFE.Insectoid">VFEI_InsectMilk</li>
					<li MayRequire="OskarPotocki.VFE.Insectoid">VFEI_EggInsectoidUnfertilized</li>
				</thingDefs>
			</li>
		</generators>
	</TG.NodeDef>
	<TG.NodeDef>
		<defName>ND_OrbitalCombatOrdnance</defName>
		<generators>
			<!-- Sells -->
			<li Class="StockGenerator_ReinforcedBarrels">
				<countRange>10~15</countRange>
			</li>
			<li Class="StockGenerator_Tag">
				<tradeTag>MortarShell</tradeTag>
				<thingDefCountRange>2~3</thingDefCountRange>
				<countRange>30~50</countRange>
			</li>
			<li Class="TG.StockGen.Grenades">
				<minTechLevelGenerate>Industrial</minTechLevelGenerate>
				<maxTechLevelGenerate>Spacer</maxTechLevelGenerate>
				<thingDefCountRange>3~4</thingDefCountRange>
				<totalPriceRange>400~1200</totalPriceRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Chemfuel</thingDef>
				<countRange>200~500</countRange>
			</li>
		</generators>
	</TG.NodeDef>
	<TG.NodeDef>
		<defName>ND_OrbitalCombatHiTech</defName>
		<generators>
			<!-- Sells -->
			<li Class="TG.StockGen.Armor">
				<minTechLevelGenerate>Industrial</minTechLevelGenerate>
				<tradeTags>
					<li>HiTechArmor</li>
					<!-- VFEP_Apparel_Privateer lacks the HiTechArmor trade tag used by other power armor items. -->
					<li MayRequire="OskarPotocki.VFE.Pirates">PrivateerArmor</li>
					<!--
					Some Vanilla Armor Expanded power armor lacks the HiTechArmor tradeTag. A new tradeTag is patched into them
					to avoid changing other behavior.
					-->
					<li MayRequire="VanillaExpanded.VARME">TG_PatchedHiTechArmor</li>
				</tradeTags>
				<thingDefCountRange>2~3</thingDefCountRange>
				<countRange>1</countRange>
			</li>
			<li Class="TG.StockGen.WeaponRanged">
				<weaponTags>
					<li>SpacerGun</li>
				</weaponTags>
				<excludeWeaponTags>
					<li MayRequire="VanillaExpanded.VWE">ToolDecent</li>
					<li MayRequire="OskarPotocki.VFE.Pirates">WarcasketAll</li>
				</excludeWeaponTags>
				<thingDefCountRange>4~7</thingDefCountRange>
				<countRange>1~2</countRange>
			</li>
			<li Class="StockGenerator_WeaponsMelee" MayRequire="Ludeon.RimWorld.Royalty">
				<weaponTag>UltratechMelee</weaponTag>
				<countRange>-1~2</countRange>
			</li>
		</generators>
	</TG.NodeDef>
</Defs>